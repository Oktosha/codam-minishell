{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Pretty gdb",
			"request": "launch",
			"type": "cppdbg",
			"program": "${workspaceRoot}/test/ps_tests/ps_tests",
			"cwd": "${workspaceFolder}/debug",
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"setupCommands": [
				{
				  "description": "Enable pretty-printing for gdb",
				  "text": "-enable-pretty-printing",
				  "ignoreFailures": false
				},
			  ],
		},
	]
}